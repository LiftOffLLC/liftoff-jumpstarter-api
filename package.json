{
  "name": "wb4server",
  "version": "1.0.0",
  "description": "Liftoff Jumpstart v1.0 REST API Server",
  "main": "build/index.js",
  "scripts": {
    "format": "./bin/codeformat.sh",
    "lint": "eslint src",
    "inspect": "jsinspect -t 30 --ignore 'src/main/database/migrations/20161006232820_users.js' src/main",
    "dev": "./bin/dev.sh",
    "clean": "rm -rf build",
    "build": "npm run clean && babel src/main -d build && npm run copy:blacklistFile && npm run copy:public && npm run copy:views",
    "worker": "node -r babel-core/register src/main/worker.js",
    "copy:blacklistFile": "cp src/main/app/commons/validators/disposable_email_blacklist.conf build/app/commons/validators/disposable_email_blacklist.conf",
    "copy:views": "cp -r src/main/public build/public",
    "copy:public": "cp -r src/main/app/views build/app/views",
    "start": "npm run build; pm2 delete all; node build/pm2.js",
    "db:migrate": "knex --knexfile ./src/main/knexfile.js migrate:latest",
    "db:rollback": "knex --knexfile ./src/main/knexfile.js migrate:rollback",
    "db:seed": "knex --knexfile ./src/main/knexfile.js seed:run",
    "test": "NODE_ENV=test lab src/test -c -r console -v -I '__core-js_shared__'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LiftOffLLC/liftoff-jumpstarter-api.git"
  },
  "keywords": [
    "liftoff",
    "quickstarter",
    "rest-api",
    "server"
  ],
  "author": "Manjunatha N",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LiftOffLLC/liftoff-jumpstarter-api/issues"
  },
  "homepage": "https://github.com/LiftOffLLC/liftoff-jumpstarter-api#readme",
  "dependencies": {
    "@hapi/boom": "^8.0.1",
    "@hapi/catbox": "^10.2.3",
    "@hapi/catbox-redis": "^5.0.5",
    "@hapi/hoek": "^8.5.0",
    "@hapi/joi": "^16.1.7",
    "bcrypt": "^3.0.6",
    "blipp": "^4.0.1",
    "bluebird": "^3.7.1",
    "checkit": "^0.7.0",
    "dataobject-parser": "^1.2.1",
    "dotenv": "^8.2.0",
    "ejs": "^2.7.1",
    "email-templates": "^6.0.3",
    "good": "^8.1.2",
    "good-console": "^8.0.0",
    "good-squeeze": "^5.1.0",
    "google-libphonenumber": "^3.2.6",
    "hapi": "^18.1.0",
    "hapi-auth-jwt2": "^8.8.0",
    "hapi-swagger": "^10.3.0",
    "hapijs-status-monitor": "^0.6.0",
    "immutable": "^4.0.0-rc.12",
    "inert": "^5.1.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.1",
    "kue": "^0.11.6",
    "lodash": "^4.17.15",
    "mrhorse": "^4.0.1",
    "nes": "^10.0.2",
    "newrelic": "^6.1.0",
    "node-sass": "^4.13.0",
    "node-uuid": "^1.4.7",
    "nodemailer": "^6.3.1",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "nodemailer-ses-transport": "^1.5.0",
    "oauth": "^0.9.15",
    "objection": "^1.6.11",
    "pg": "^7.12.1",
    "pm2": "^4.1.2",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "require-dir": "^1.2.0",
    "toposort": "^2.0.2",
    "validator": "^12.0.0",
    "vision": "^5.4.4",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.26.0",
    "code": "^5.2.4",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-eslint": "^5.0.1",
    "eslint-plugin-hapi": "^4.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsdoc": "^17.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-lodash": "^6.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-security": "^1.3.0",
    "js-beautify": "^1.10.2",
    "jsinspect": "^0.12.7",
    "lab": "^18.0.2",
    "nodemon": "^1.19.4"
  },
  "engines": {
    "node": "<=12"
  }
}
