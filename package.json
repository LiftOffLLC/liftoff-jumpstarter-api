{
  "name": "wb4server",
  "version": "1.0.0",
  "description": "Liftoff Jumpstart v1.0 REST API Server",
  "main": "build/index.js",
  "scripts": {
    "format": "./bin/codeformat.sh",
    "lint": "eslint src",
    "inspect": "jsinspect -t 30 --ignore 'src/main/migrations/20161006232820_users.js' src/main",
    "dev": "./bin/dev.sh",
    "clean": "rm -rf build",
    "build": "npm run clean && babel src/main -d build && npm run copy:blacklistFile && npm run copy:public && npm run copy:views",
    "worker": "node -r babel-core/register src/main/worker.js",
    "copy:blacklistFile": "cp src/main/app/commons/disposable_email_blacklist.conf build/app/commons/disposable_email_blacklist.conf",
    "copy:views": "cp -r src/main/public build/public",
    "copy:public": "cp -r src/main/app/views build/app/views",
    "start": "npm run build; pm2 delete all; node build/pm2.js",
    "db:migrate": "knex --knexfile ./src/main/knexfile.js migrate:latest",
    "db:rollback": "knex --knexfile ./src/main/knexfile.js migrate:rollback",
    "db:seed": "knex --knexfile ./src/main/knexfile.js seed:run",
    "test": "NODE_ENV=test lab src/test -c -r console -v -I '__core-js_shared__'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LiftOffLLC/liftoff-jumpstarter-api.git"
  },
  "keywords": [
    "liftoff",
    "quickstarter",
    "rest-api",
    "server"
  ],
  "author": "Manjunatha N",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LiftOffLLC/liftoff-jumpstarter-api/issues"
  },
  "homepage": "https://github.com/LiftOffLLC/liftoff-jumpstarter-api#readme",
  "dependencies": {
    "bcrypt": "^0.8.7",
    "blipp": "^2.3.0",
    "bluebird": "^3.4.7",
    "boom": "^4.2.0",
    "catbox-redis": "^2.0.2",
    "checkit": "^0.7.0",
    "dataobject-parser": "^1.1.3",
    "email-templates": "^2.5.4",
    "good": "^7.1.0",
    "good-console": "^6.2.0",
    "good-squeeze": "^5.0.1",
    "google-libphonenumber": "^2.0.7",
    "hapi": "^16.1.0",
    "hapi-auth-jwt2": "^7.2.4",
    "hapi-swagger": "^7.5.0",
    "hapijs-status-monitor": "^0.5.0",
    "immutable": "^3.8.1",
    "inert": "^4.1.0",
    "joi": "^10.2.0",
    "jsonwebtoken": "^7.2.1",
    "knex": "^0.12.6",
    "kue": "^0.11.5",
    "lodash": "^4.17.4",
    "mrhorse": "^2.1.0",
    "nes": "^6.4.2",
    "newrelic": "^1.36.0",
    "node-sass": "^4.2.0",
    "node-uuid": "^1.4.7",
    "nodemailer": "^2.7.0",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "nodemailer-ses-transport": "^1.5.0",
    "oauth": "^0.9.15",
    "objection": "^0.6.2",
    "pg": "^6.1.2",
    "pm2": "^2.4.2",
    "redis": "^2.6.3",
    "request": "^2.79.0",
    "require-dir": "^0.3.1",
    "toposort": "^1.0.0",
    "validator": "^6.2.1",
    "vision": "^4.1.1",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-regenerator": "^6.16.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "code": "^4.0.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "js-beautify": "^1.6.4",
    "jsinspect": "^0.8.0",
    "lab": "^11.1.0",
    "nodemon": "^1.10.2"
  },
  "engines": {
    "node": "6.9.1"
  }
}
