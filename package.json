{
  "name": "liftoff-jumpstarter-api",
  "version": "2.1.0",
  "description": "Liftoff Jumpstarter v2.1 REST API Server",
  "main": "src/main/index.js",
  "scripts": {
    "lint": "eslint . --ext .js,.ts --ignore-path .gitignore",
    "lint:fix": "yarn lint --fix",
    "inspect": "jsinspect -t 30 --ignore 'src/main/database/migrations/20161006232820_users.js' src/main",
    "dev": "./bin/dev.sh",
    "worker": "node src/main/worker.js",
    "start": "pm2 delete all; node src/main/pm2.js",
    "db:migrate": "knex --knexfile ./src/main/knexfile.js migrate:latest",
    "db:rollback": "knex --knexfile ./src/main/knexfile.js migrate:rollback",
    "db:seed": "knex --knexfile ./src/main/knexfile.js seed:run",
    "test": "NODE_ENV=test lab src/test -c -r console -v -I '__core-js_shared__'",
    "format": "prettier --write '**/*.{ts,js,css,html}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LiftOffLLC/liftoff-jumpstarter-api.git"
  },
  "keywords": [
    "liftoff",
    "quickstarter",
    "rest-api",
    "server"
  ],
  "author": "LiftOff LLC",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LiftOffLLC/liftoff-jumpstarter-api/issues"
  },
  "homepage": "https://github.com/LiftOffLLC/liftoff-jumpstarter-api#readme",
  "lint-staged": {
    "*.js": [
      "yarn lint",
      "prettier --write",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@hapi/boom": "^9.1.0",
    "@hapi/catbox-redis": "^5.0.5",
    "@hapi/good": "^9.0.0",
    "@hapi/good-squeeze": "^6.0.0",
    "@hapi/hapi": "^19.1.1",
    "@hapi/inert": "^6.0.1",
    "@hapi/joi": "^17.1.1",
    "@hapi/joi-date": "^2.0.1",
    "@hapi/nes": "^12.0.2",
    "@hapi/vision": "^6.0.0",
    "bcrypt": "^5.0.0",
    "blipp": "^4.0.1",
    "bluebird": "^3.7.2",
    "bull": "^3.14.0",
    "checkit": "^0.7.0",
    "dataobject-parser": "^1.2.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.2",
    "email-templates": "^7.0.4",
    "google-libphonenumber": "^3.2.9",
    "hapi-auth-jwt2": "^10.1.0",
    "hapi-sentry": "^3.0.0",
    "hapi-swagger": "^13.0.2",
    "hapijs-status-monitor": "^1.0.1",
    "has-ansi": "^4.0.0",
    "husky": "4.2.5",
    "immutable": "^4.0.0-rc.12",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.1",
    "lodash": "^4.17.15",
    "mrhorse": "^6.0.0",
    "newrelic": "^6.7.1",
    "node-sass": "^4.14.0",
    "node-uuid": "^1.4.7",
    "nodemailer": "^6.4.6",
    "nodemailer-ses-transport": "^1.5.0",
    "objection": "^2.1.3",
    "password-validator": "^5.0.3",
    "pg": "^8.2.1",
    "pm2": "^4.4.0",
    "redis": "^3.0.2",
    "request": "^2.88.2",
    "require-dir": "^1.2.0",
    "toposort": "^2.0.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "code": "^5.2.4",
    "eslint": "7.2.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-hapi": "^4.1.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "4.0.0",
    "eslint-plugin-security": "^1.3.0",
    "jsinspect": "^0.12.7",
    "lab": "^18.0.2",
    "lint-staged": "^10.2.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  },
  "engines": {
    "node": "12"
  }
}
